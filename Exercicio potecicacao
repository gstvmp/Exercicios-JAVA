import java.util.Locale;
import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
	
		Locale.setDefault(Locale.US);
		Scanner sc = new Scanner(System.in);
		
		double R,pi,A;
		R = sc.nextDouble();
		pi = 3.14159;
		A = pi * R * R;
		
		System.out.printf("area=%.4f%n", A);
		
	sc.close();
	}
}
